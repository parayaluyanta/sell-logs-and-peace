{
  "configuration": {
    "name": "logggg",
    "appenders": {
      "RollingFile": {
        "name":"rollingStone",
        "fileName":"supply_chain_rolled.log",
        "filePattern":"%d{MM-dd-yy-HH-mm-ss}-%i.log.gz",
        "JSONLayout": {
          "complete" : true
        },
        "Policies": {
          "SizeBasedTriggeringPolicy": {
            "size":"10 MB"
          }
        },
        "DefaultRolloverStrategy": {
          "max":"10"
        }
      }
    },
    "loggers": {
      "root": {
        "level":"debug",
        "appender-ref": {
          "ref":"rollingStone"
        }
      }
    }
  }
}